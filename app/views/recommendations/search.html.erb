<div class="page-header">
  <h1>  Search Results </h1>
</div>
<div class="recommendations-list">
 <% @search_results.each do |recommendation|  %>
  <ul class="recommendation">
    <li>
      <h3>
      <%= "#{recommendation.title} : #{recommendation.description} : #{recommendation.url} : #{ recommendation.tag}"%>
      </h3>
    </li>
  </ul>
  <% end %>
</div>

